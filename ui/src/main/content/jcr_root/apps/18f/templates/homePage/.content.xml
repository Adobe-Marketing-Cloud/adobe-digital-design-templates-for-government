<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Template"
          jcr:title="18F Home Page"
          allowedChildren="[/apps/18f/templates/contentPage]"
          allowedPaths="[/content]"
          ranking="{Long}100">
    <jcr:content
            cq:template="/apps/18f/templates/homePage"
            jcr:primaryType="cq:PageContent"
            jcr:title="18fhome"
            sling:resourceType="18f/components/page"
            hideFromBreadcrumb="true"
            pageLayout="usa-grid-full"
            showFooter="show"
            showHeader="show"
            showSidenav="hide">
        <footerparsys
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/iparsys">
            <footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="18f/components/footer"
                    footerType="medium">
                <bgImage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/image"
                        imageRotate="0"/>
                <footerleft
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/parsys">
                    <grid
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="18f/components/grid"
                            columnConfiguration="o2x2x2x2x2x2"
                            columnfive="2"
                            columnfour="2"
                            columnone="2"
                            columnsix="2"
                            columnsselect="6"
                            columnthree="2"
                            columntwo="2"
                            isBorderBottom="{Boolean}false"
                            isBorderInside="{Boolean}false"
                            isBorderTop="{Boolean}false"
                            matchColumnHeight="true">
                        <column0
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="foundation/components/parsys">
                            <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="18f/components/text"
                                    text="&lt;p>&lt;a href=&quot;https://playbook.cio.gov/designstandards/footers/&quot;>Primary Link&lt;/a>&lt;/p>&#xa;"
                                    textIsRich="true"/>
                        </column0>
                        <column1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="foundation/components/parsys">
                            <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="18f/components/text"
                                    text="&lt;p>&lt;a href=&quot;https://playbook.cio.gov/designstandards/footers/&quot;>Primary Link&lt;/a>&lt;/p>&#xa;"
                                    textIsRich="true"/>
                        </column1>
                        <column2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="foundation/components/parsys">
                            <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="18f/components/text"
                                    text="&lt;p>&lt;a href=&quot;https://playbook.cio.gov/designstandards/footers/&quot;>Primary Link&lt;/a>&lt;/p>&#xa;"
                                    textIsRich="true"/>
                        </column2>
                        <column3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="foundation/components/parsys">
                            <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="18f/components/text"
                                    text="&lt;p>&lt;a href=&quot;https://playbook.cio.gov/designstandards/footers/&quot;>Primary Link&lt;/a>&lt;/p>&#xa;"
                                    textIsRich="true"/>
                        </column3>
                        <column4
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="foundation/components/parsys">
                            <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="18f/components/text"
                                    text="&lt;p style=&quot;font-family: tahoma, arial, helvetica, sans-serif; font-size: 12px;&quot;>(800) CALL-GOVT&lt;/p>&#xa;"
                                    textIsRich="true"/>
                        </column4>
                        <column5
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="foundation/components/parsys">
                            <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="18f/components/text"
                                    text="&lt;p>&lt;a href=&quot;mailto:info@agency.gov&quot;>info@agency.gov&lt;/a>&lt;/p>&#xa;"
                                    textIsRich="true"/>
                        </column5>
                    </grid>
                </footerleft>
            </footer>
        </footerparsys>
        <headerparsys
            jcr:primaryType="nt:unstructured"
            sling:resourceType="foundation/components/iparsys">
            <grid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="18f/components/grid"
                columnConfiguration="o3x6x3"
                columnfive="1"
                columnfour="1"
                columnone="3"
                columnsix="1"
                columnsselect="3"
                columnthree="3"
                columntwo="6"
                isBorderBottom="{Boolean}false"
                isBorderInside="{Boolean}false"
                isBorderTop="{Boolean}false"
                matchColumnHeight="true">
                <column0
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="foundation/components/parsys">
                    <image
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="foundation/components/image"
                        imageRotate="0">
                        <file/>
                    </image>
                </column0>
                <column1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="foundation/components/parsys">
                    <text
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="18f/components/text"
                        text="&lt;h2>Agency Name&lt;/h2>&#xa;"
                        textIsRich="true"/>
                </column1>
                <column2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="foundation/components/parsys">
                    <sociallinks
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="18f/components/sociallinks"
                        facebookEnabled="true"
                        rssEnabled="true"
                        twitterEnabled="true"
                        youtubeEnabled="true"/>
                </column2>
            </grid>
        </headerparsys>
    </jcr:content>
</jcr:root>
